<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nepo">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logonepo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logonepo"
        android:supportsRtl="true"
        android:theme="@style/Theme.NEPO">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".OpenRecActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".FormActivity"
            android:parentActivityName=".ListFormActivity" />
        <activity android:name=".KegiatanActivity"
            android:parentActivityName=".OpenRecActivity"/>
        <activity android:name=".ListFormActivity"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".DetailFormActivity"/>
    </application>

</manifest>